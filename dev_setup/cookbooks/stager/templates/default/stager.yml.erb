nats_uri: nats://<%= node[:nats_server][:user] %>:<%= node[:nats_server][:password] %>@<%= node[:nats_server][:host] %>:<%= node[:nats_server][:port] %>/
logging:
  level: debug

pid_filename: /var/vcap/sys/run/stager.pid

max_staging_duration: 120
max_active_tasks: 10

queues:
  - staging

dirs:
  manifests: <%= File.join(node[:deployment][:config_path], "legacy_staging") %>
  tmp: /var/vcap/data/stager/tmp

ruby_path: <%= File.join(node[:ruby][:path], "bin", "ruby")%>

secure: false
