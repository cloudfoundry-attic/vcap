---
cloud_controller_uri: <%= node[:cloud_controller][:service_api_uri] %>
service:
  name: mssql
  version: "2008"
  description: 'MS SQL Server database service'
  plans: ['free']
  tags: ['mssql', '2008', 'relational']
host: localhost
index: 0
token: <%= node[:mssql_node][:token] %>
mbus: nats://<%= node[:nats_server][:user] %>:<%= node[:nats_server][:password] %>@<%= node[:nats_server][:host] %>:<%= node[:nats_server][:port] %>/
pid: /var/vcap/sys/run/mssql_service.pid
node_timeout: 60
logging:
  level: debug