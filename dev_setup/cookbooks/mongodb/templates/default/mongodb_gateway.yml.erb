---
cloud_controller_uri: <%= node[:cloud_controller][:service_api_uri] %>
service:
  name: mongodb
  version: "1.8"
  description: 'MongoDB NoSQL store'
  plans: ['free']
  tags: ['mongodb', 'mongodb-1.6', 'nosql', 'document']
host: localhost
index: 0
token: <%= node[:mongodb_node][:token] %>
mbus: nats://<%= node[:nats_server][:user] %>:<%= node[:nats_server][:password] %>@<%= node[:nats_server][:host] %>:<%= node[:nats_server][:port] %>/
pid: /var/vcap/sys/run/mongodb_service.pid
logging:
  level: debug
# allow_over_provisioning: false
