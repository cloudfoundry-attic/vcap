---
name: "standalone"
runtimes:
  - python2.current:
      environment:
        path: /usr/bin:$PATH
  - java6.current:
      environment:
        path: /usr/bin:$PATH
  - node04.current:
      environment:
        path: <%= File.join(node[:node04][:path], "bin") %>:$PATH
  - node06.current:
      environment:
        path: <%= File.join(node[:node06][:path], "bin") %>:$PATH
  - erlang.current:
      environment:
        path: <%= File.join(node[:erlang][:path], "bin") %>:$PATH
  - php5.current:
      environment:
        path: /usr/bin:$PATH
  - ruby18.current:
      environment:
        path: <%= File.join(node[:ruby18][:path], "bin") %>:$PATH
        bundle_gemfile:
  - ruby19.current:
      environment:
        path: <%= File.join(node[:ruby][:path], "bin") %>:$PATH
        bundle_gemfile:

