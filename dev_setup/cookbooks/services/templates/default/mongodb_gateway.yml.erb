---
# cloud_controller_uri: api.vcap.me
service:
  name: mongodb
  version: "1.8"
  description: 'MongoDB NoSQL store'
  plans: ['free']
  tags: ['mongodb', 'mongodb-1.6', 'nosql']
host: localhost
index: 0
token: <%= node[:service][:mongodb][:token] %>
mbus: nats://<%= node[:nats][:user] %>:<%= node[:nats][:password] %>@<%= node[:nats][:host] %>:<%= node[:nats][:port] %>/
log_level: DEBUG
pid: /var/vcap/sys/run/mongodb_service.pid
# allow_over_provisioning: false
