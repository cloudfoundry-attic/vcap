---
cloud_controller_uri: <%= node[:cloud_controller][:service_api_uri] %>
service:
  name: mysql
  version: "5.1"
  description: 'MySQL database service'
  plans: ['free']
  tags: ['mysql', 'mysql-5.1', 'relational']
host: localhost
index: 0
token: <%= node[:service][:mysql][:token] %>
log_level: DEBUG
mbus: nats://<%= node[:nats][:user] %>:<%= node[:nats][:password] %>@<%= node[:nats][:host] %>:<%= node[:nats][:port] %>/
pid: /var/vcap/sys/run/mysql_service.pid
node_timeout: 2
