---
# cloud_controller_uri: api.vcap.me
service:
  name: redis
  version: "2.2"
  description: 'Redis key-value store service'
  plans: ['free']
  tags: ['redis', 'redis-2.2', 'key-value', 'nosql']
host: localhost
index: 0
token: <%= node[:service][:redis][:token] %>
log_level: DEBUG
mbus: nats://<%= node[:nats][:user] %>:<%= node[:nats][:password] %>@<%= node[:nats][:host] %>:<%= node[:nats][:port] %>/
pid: /var/vcap/sys/run/redis_service.pid
# allow_over_provisioning: false
