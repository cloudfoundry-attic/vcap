---
cloud_controller_uri: <%= node[:cloud_controller][:service_api_uri] %>
service:
  name: postgresql
  version: "9.0"
  description: 'PostgreSQL database service'
  plans: ['free']
  tags: ['postgresql', 'postgresql-9.0', 'relational']
host: localhost
token: <%= node[:service][:postgresql][:token] %>
log_level: DEBUG
mbus: nats://<%= node[:nats][:user] %>:<%= node[:nats][:password] %>@<%= node[:nats][:host] %>:<%= node[:nats][:port] %>/
pid: /var/vcap/sys/run/postgresql_service.pid
node_timeout: 2
