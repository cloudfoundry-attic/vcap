---
cloud_controller_uri: <%= node[:cloud_controller][:service_api_uri] %>
service:
  name: neo4j
  version: "1.4"
  description: 'Neo4j NOSQL store'
  plans: ['free']
  default_plan: 'free'
  tags: ['nosql']
  timeout: <%= node[:neo4j_gateway][:service][:timeout] %>
  supported_versions: <%= "['#{node[:neo4j][:supported_versions].keys.join("', '")}']"%>
  version_aliases:
<% node[:neo4j][:version_aliases].each do |a, v| %>
    "<%= a %>": "<%= v %>"
<% end %>
host: localhost
index: 0
token: <%= node[:neo4j_node][:token] %>
logging:
  level: debug
mbus: nats://<%= node[:nats_server][:user] %>:<%= node[:nats_server][:password] %>@<%= node[:nats_server][:host] %>:<%= node[:nats_server][:port] %>/
pid: /var/vcap/sys/run/neo4j_service.pid
node_timeout: <%= node[:neo4j_gateway][:node_timeout] %>
